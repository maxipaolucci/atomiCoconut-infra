{
	"agent": {
		"metrics_collection_interval": 60,
		"run_as_user": "root"
	},
	"logs": {
		"logs_collected": {
			"files": {
				"collect_list": [
					{
						"file_path": "/var/log/eb-docker/containers/eb-current-app/eb-stdouterr.log",
						"log_group_name": "aco-ENVIRONMENT_TYPE-containers-logs",
						"log_stream_name": "{instance_id}"
					}
				]
			}
		}
	}
}