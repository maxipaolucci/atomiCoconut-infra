{
	"agent": {
		"metrics_collection_interval": 60,
		"run_as_user": "root"
	},
	"logs": {
		"logs_collected": {
			"files": {
				"collect_list": [
					{
						"file_path": "/var/log/containers/server-CONTAINER_ID-stdouterr.log",
						"log_group_name": "aco-server-container-logs",
						"log_stream_name": "{instance_id}"
					}
				]
			}
		}
	}
}